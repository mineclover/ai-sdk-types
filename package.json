{
  "name": "@mineclover/ai-sdk-types",
  "version": "0.1.0",
  "description": "Shared type definitions for AI SDK communication across packages",
  "author": "mineclover",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mineclover/webs.git",
    "directory": "packages/ai-sdk-types"
  },
  "homepage": "https://github.com/mineclover/webs/tree/main/packages/ai-sdk-types",
  "bugs": {
    "url": "https://github.com/mineclover/webs/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./messages": {
      "types": "./dist/messages.d.ts",
      "import": "./dist/messages.js",
      "require": "./dist/messages.cjs"
    },
    "./usage": {
      "types": "./dist/usage.d.ts",
      "import": "./dist/usage.js",
      "require": "./dist/usage.cjs"
    },
    "./providers": {
      "types": "./dist/providers.d.ts",
      "import": "./dist/providers.js",
      "require": "./dist/providers.cjs"
    },
    "./stream": {
      "types": "./dist/stream.d.ts",
      "import": "./dist/stream.js",
      "require": "./dist/stream.cjs"
    },
    "./mcp": {
      "types": "./dist/mcp.d.ts",
      "import": "./dist/mcp.js",
      "require": "./dist/mcp.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "sdk",
    "types",
    "typescript",
    "openai",
    "anthropic",
    "claude"
  ],
  "license": "MIT",
  "dependencies": {
    "@packages/zod-schema": "github:mineclover/zod-schema"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  }
}
